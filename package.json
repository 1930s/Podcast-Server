    "gulp-conventional-changelog": "^0.7.0",
    "gulp-eslint": "^1.0.0",
    "gulp-flatten": "^0.2.0",
    "gulp-git": "^1.6.0",
    "gulp-gzip": "^1.2.0",
    "gulp-inject": "^3.0.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-ng-annotate": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.2",
    "gulp-uncss": "^1.0.4",
    "gulp-util": "^3.0.7",
    "http-server": "^0.8.5",
    "isparta": "^3.1.0",
    "istanbul": "gotwarlost/istanbul.git#source-map",
    "jasmine-core": "^2.3.4",
    "jasmine-expect": "^2.0.0-beta2",
    "jasmine-reporters": "^2.0.7",
    "jspm": "^0.16.13",
    "mkdirp": "^0.5.1",
    "proxy-middleware": "^0.15.0",
